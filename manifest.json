{
  "name": "Endless Spotify",
  "description": "Makes sure spotify never stops",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://*.spotify.com/*"
]
}